Algoritmo "exercicioCONSUMO"


Var

   distancia : inteiro
   combustivel : real
   consumo : real


Inicio

   escreva("Distância percorrida: ")
   leia(distancia)
   escreva("Combustível gasto: ")
   leia(combustivel)

   
   consumo <- distancia / combustivel

   escreva("Consumo médio :" , consumo:3:3)
Fimalgoritmo